package com.hackbulgaria.corejava;

import java.util.LinkedList;
import java.util.List;

public class Expr {

	List<Object> ll;
	
	public Expr(){
		List ll = new LinkedList();
		this.ll=ll;
	}
	
	public void add(Object e) {
		ll.add(e);
	}
	

	
	public static void main(String[] args) {
		Expr e = new Expr();
		e.add(1);
		
		
	}
}
